<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>
        List Of Parties
    </h1>
    <table border="1" >
     <tr>
         <th>Party Name</th>
         <th>Main Candidate</th>
         <th>Short Name</th>
     </tr>
     <% parties.forEach(function(item){ %>
        <tr>
            <td><%=item.name%></td>
            <td><%=item.main_candidate%></td>
            <td><%=item.short_name%></td>
        </tr>
     <% }) %>   
    </table>
</body>
</html>